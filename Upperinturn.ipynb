    "def upperinturn(sentence):\n",
    "    result = []\n",
    "    for i in range (len (sentence)) : \n",
    "        if sentence[i].isalpha():\n",
    "            result.append(sentence[:i] + sentence[i].upper() + sentence[i +1 :])\n",
    "    return result\n",
    "upperinturn( \"İstanbul\")"
   ]
