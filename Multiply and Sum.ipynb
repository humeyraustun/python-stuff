   "source": [
    "def mult_sum(list):\n",
    "    sum = 0\n",
    "    for i in range(0,len(list)):\n",
    "        sum += i* list[i]\n",
    "    return sum\n",
    "mult_sum ([1,2])"
   ]
  }
