    "def eliminate (word):\n",
    "    new = \" \"\n",
    "    for i in range(len(word)-1):\n",
    "        if word [i] != word [i+1] :\n",
    "            new += word[i]\n",
    "    new += word[-1]\n",
    "    return new\n",
    "print(eliminate(\"hhhhhhhhhiiiiiiiii\"))   "
