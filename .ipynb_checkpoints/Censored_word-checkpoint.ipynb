    "def censor(censored, vowels):\n",
    "    n=0\n",
    "    for i in censored:\n",
    "        if i ==\"*\":\n",
    "            censored= censored.replace (\"*\", vowels[n],1)\n",
    "            n+=1\n",
    "    return censored\n",
    "print (censor (\"pyth*n\", \"o\"))\n",
    "            "
   ]
  }
