    "def settletheworld(list1, bool):\n",
    "    edited = []\n",
    "    for i in list1:\n",
    "        edited.append(i)\n",
    "        edited.append(\"Pong\")\n",
    "    if bool:\n",
    "        return edited\n",
    "    else: \n",
    "        del edited [-1]\n",
    "        return edited\n",
    "    \n",
    "settletheworld([\"Ping\",\"Ping\",\"Ping\",\"Ping\"],True)\n",
    "        "
   ]
